{
  "name": "another-chance-api",
  "version": "1.0.0",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "8.1.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "description": "Another chance",
  "main": "app.js",
  "scripts": {
    "start": "node ./server.js",
    "test": "istanbul cover --include-all-sources --report lcov -- _mocha test/**/*.test.js -R spec  --compilers js:babel-core/register",
    "coverage": "bin/coverage.sh",
    "coveralls": "npm run test -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "dev-server": "nodemon ./server.js",
    "dev": "webpack -w",
    "heroku-postbuild": "npm install --only=dev && webpack -p --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glenrage/another-chance.git"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "1.17.2",
    "cors": "2.8.4",
    "dotenv": "^4.0.0",
    "ejs": "2.5.7",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.6.0",
    "errorhandler": "1.5.0",
    "express": "4.15.3",
    "express-jwt": "5.3.0",
    "express-session": "1.15.5",
    "jest": "^20.0.4",
    "jsonwebtoken": "7.4.2",
    "method-override": "2.3.9",
    "methods": "1.1.2",
    "mongoose": "4.11.5",
    "mongoose-unique-validator": "1.0.5",
    "morgan": "1.8.2",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-dropzone": "^3.13.4",
    "react-notification-system": "^0.2.16",
    "react-redux": "^4.4.8",
    "react-router": "^3.2.0",
    "react-test-renderer": "^15.6.2",
    "redux": "^3.6.0",
    "request": "2.81.0",
    "slug": "0.9.1",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^2.13.3",
    "cross-env": "^5.1.3",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.14.12",
    "nyc": "^11.1.0",
    "webpack": "^3.10.0"
  }
}
